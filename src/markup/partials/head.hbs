<head>
    <meta charset="UTF-8"/>
    <meta content="IE=edge" http-equiv="X-UA-Compatible"/>
    <meta content="width=device-width, initial-scale=1.0" name="viewport"/>

    {{#block "title"}}
        <title>{{title}}</title>
    {{/block}}

    {{#block "styles"}}
        {{#each stylesheets}}
            <link href="{{this}}" rel="stylesheet"/>
        {{/each}}
    {{/block}}
</head>
