{
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "max-len": [
      2,
      120
    ],
    "no-console": "off",
    "class-methods-use-this": "off",
    "no-underscore-dangle": "off"

  },
  "env": {
    "browser": true,
    "node": true
  }
}
